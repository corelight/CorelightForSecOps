                },
                {
                    "name": "e5aba11d-a760-4c8a-b1a0-7bd60e5f6454",
                    "query": "metadata.vendor_name=\"Corelight\"\r\nmetadata.product_event_type = \"software\" \r\n$software_type=target.asset.software.description\r\n$software_type!=\"\"\r\nmatch:\r\n    $software_type\r\noutcome:\r\n    $count=count_distinct(metadata.id)\r\norder:\r\n    $count desc \r\nlimit:\r\n    10",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "DAY",
                            "startTimeVal": "1"
                        }
                    },
                    "etag": "1276b346a640e98d28ec4cbeb60c47332548021cb4449dec7515409171144203"
                },
                {
                    "name": "83a824e8-bc44-4078-8ada-4bf7c6f76bfb",
                    "query": "metadata.vendor_name=\"Corelight\"\r\nmetadata.product_event_type = \"software\" \r\n$name=target.asset.software.name\r\n$name!=\"\"\r\n$version= strings.concat(if(target.asset.software.version!=\"\", target.asset.software.version, \"\"), \r\n          if(target.asset.attribute.labels[\"version_minor\"]!=\"\", strings.concat(\".\", target.asset.attribute.labels[\"version_minor\"]), \"\"), \r\n          if(target.asset.attribute.labels[\"version_minor2\"]!=\"\", strings.concat(\".\", target.asset.attribute.labels[\"version_minor2\"]), \"\"))\r\nmatch:\r\n    $name, $version\r\noutcome:\r\n    $count=count_distinct(metadata.id)\r\norder:\r\n    $count desc \r\nlimit:\r\n    10",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "DAY",
                            "startTimeVal": "1"
                        }
                    },
                    "etag": "933f2caa18763e5f1081d3430f099652a643ef01eca6aaf8006f91680bfe4524"
                },
                {
                    "name": "d8b0a08e-e112-4b0a-bbfc-ff8e2ebb274c",
                    "query": "metadata.vendor_name=\"Corelight\"\r\nmetadata.product_event_type = \"software\" \r\n$time=timestamp.get_timestamp(metadata.event_timestamp.seconds)\r\n$version= strings.concat(if(target.asset.software.version!=\"\", target.asset.software.version, \"\"), \r\n          if(target.asset.attribute.labels[\"version_minor\"]!=\"\", strings.concat(\".\", target.asset.attribute.labels[\"version_minor\"]), \"\"), \r\n          if(target.asset.attribute.labels[\"version_minor2\"]!=\"\", strings.concat(\".\", target.asset.attribute.labels[\"version_minor2\"]), \"\"))\r\n$version_details=target.asset.attribute.labels[\"version_addl\"]\r\nmatch:\r\n    $time, observer.hostname, target.asset.ip, target.asset.software.name, $version, $version_details, target.asset.software.description\r\norder:\r\n    $time desc",
                    "input": {
