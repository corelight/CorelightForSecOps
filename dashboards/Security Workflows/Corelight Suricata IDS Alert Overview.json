            "dashboardQueries": [
                {
                    "name": "347d1e89-6b87-401e-a016-e8f1a8bb7d5e",
                    "query": "metadata.vendor_name=\"Corelight\"\r\nmetadata.product_event_type = \"suricata_corelight\"\r\nprincipal.ip!=\"\"\r\n$severity=if(security_result.severity_details=\"Informational\",\"Informational\",\r\n          if(security_result.severity_details=\"Critical\",\"Critical\",\r\n          if(security_result.severity_details=\"Major\",\"Medium\",\r\n          if(security_result.severity_details=\"Minor\", \"Low\", \"Unknown\"))))\r\n$severity!=\"Unknown\"\r\nsecurity_result.category_details!=\"\"\r\n$time=timestamp.get_timestamp(metadata.event_timestamp.seconds)\r\n$service=about.labels[\"service\"]\r\n$uid=about.labels[\"uid\"]\r\nmatch:\r\n    $time, principal.ip, target.ip, target.port, $service, $severity, security_result.rule_name, security_result.rule_id, $uid\r\norder:\r\n    $time desc",
                    "input": {
                        "relativeTime": {
                            "timeUnit": "DAY",
                            "startTimeVal": "1"
                        }
                    },
                    "etag": "2293cd9910218c02411811589689d050a58c09ff10b3efcc8e674c02ebad4b4b"
                },
                {
                    "name": "8fb8d410-ba6e-4565-a82f-13193313ca5c",
                    "query": "metadata.vendor_name=\"Corelight\"\r\nmetadata.product_event_type = \"suricata_corelight\"\r\nprincipal.ip!=\"\"\r\nsecurity_result.severity_details!=\"\"\r\nsecurity_result.category_details!=\"\"\r\nmatch:\r\n    principal.ip\r\noutcome:\r\n    $signature_count=count_distinct(security_result.rule_name)\r\n    $count=count_distinct(metadata.id)\r\norder:\r\n    $signature_count desc, $count desc",
                    "input": {
                        "relativeTime": {
